#include <stdio.h>

#define MAX 5

int cq[MAX];
int front = 0, rear = -1, count = 0;

void insert() {
    if (count == MAX) {
        printf("Circular Queue Overflow\n");
        return;
    }
    int element;
    printf("Enter the value: ");
    scanf("%d", &element);
    rear = (rear + 1) % MAX;
    cq[rear] = element;
    count++;
}

void delete_ele() {
    if (count == 0) {
        printf("Circular Queue Underflow\n");
        return;
    }
    printf("Element deleted is %d\n", cq[front]);
    front = (front + 1) % MAX;
    count--;
}

void display() {
    if (count == 0) {
        printf("Circular Queue is empty\n");
        return;
    }
    printf("Contents of Circular Queue are:\n");
    int i, index = front;
    for (i = 0; i < count; i++) {
        printf("%d ", cq[index]);
        index = (index + 1) % MAX;
    }
    printf("\n");
}

int main() {
    int choice;
    do {
        printf("\n1: Insert\n2: Delete\n3: Display\n4: Exit\n");
        printf("Enter your choice: ");
        scanf("%d", &choice);

        switch (choice) {
            case 1: insert(); break;
            case 2: delete_ele(); break;
            case 3: display(); break;
            case 4: printf("Exiting program\n"); break;
            default: printf("Invalid choice\n");
        }
    } while (choice != 4);
    return 0;
}
